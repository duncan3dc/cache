parameters:
  ignoreErrors:
    - '/Call to an undefined method duncan3dc\\ObjectIntruder\\Intruder::/'

    - '/Call to protected method .* of class duncan3dc\\CacheTests\\CacheKeys/'
    - '/Call to an undefined method duncan3dc\\CacheTests\\CacheCalls::/'

    - '/Parameter #1 .* of method Psr\\SimpleCache\\CacheInterface::/'

    - '/Parameter #1 .* of method duncan3dc\\Cache\\Item::expires(At|After)\(\) expects Date[A-Za-z]+(|int)?|null, string given\./'

    - '/Parameter #1 \$exception of method PHPUnit\\Framework\\TestCase::expectException\(\) expects class-string<Throwable>, string given\.$/'

includes:
	- vendor/phpstan/phpstan-mockery/extension.neon
	- vendor/phpstan/phpstan-phpunit/extension.neon
